{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "types": ["cypress", "node"],
    "module": "ESNext",
    "strict": true,
    "esModuleInterop": true,

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": [
    "cypress/**/*.ts",
    "src/**/*.ts"
  ]
}
